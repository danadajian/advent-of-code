{
  "name": "advent-of-code",
  "devDependencies": {
    "@types/jest": "27.0.3",
    "jest": "27.4.3",
    "ts-jest": "27.0.7"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest"
    },
    "testRegex": "(\\.(test))\\.(ts)$",
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  }
}
